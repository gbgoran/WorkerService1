<Window x:Class="WPF1_ContactList.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF1_ContactList"
        mc:Ignorable="d"
        Title="Contact List" Height="450" Width="800">
    <Grid Margin="20">
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0">
            <Label Content="First Name"/>
            <TextBox x:Name="tbFirstName" Padding="10,20" Margin=""/>
            <Label Content="Last Name"/>
            <TextBox x:Name="tbLastName" Padding="10,20"/>

        </StackPanel>




    </Grid>
</Window>
